{
  "name": "electric-vehicle-battery-swap-diagnosis",
  "version": "1.0.0",
  "description": "An intelligent fault diagnosis system for electric vehicle battery swap stations, featuring knowledge graph visualization, decision tree diagnosis, and multi-modal input analysis.",
  "homepage": "https://baggio200cn.github.io/electric-vehicle-battery-swap-diagnosis",
  "repository": {
    "type": "git",
    "url": "https://github.com/Baggio200cn/electric-vehicle-battery-swap-diagnosis.git"
  },
  "keywords": [
    "electric-vehicle",
    "battery-swap",
    "fault-diagnosis",
    "knowledge-graph",
    "react",
    "typescript",
    "material-ui",
    "machine-learning",
    "ai"
  ],
  "author": "Baggio200cn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Baggio200cn/electric-vehicle-battery-swap-diagnosis/issues"
  },
  "private": false,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.65",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:gh-pages": "PUBLIC_URL=/electric-vehicle-battery-swap-diagnosis npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,css,json}",
    "predeploy": "npm run build:gh-pages",
    "deploy": "gh-pages -d build",
    "build:ci": "DISABLE_ESLINT_PLUGIN=true ESLINT_NO_DEV_ERRORS=true CI=false react-scripts build",
    "ci:build": "DISABLE_ESLINT_PLUGIN=true ESLINT_NO_DEV_ERRORS=true CI=false react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
